{
    "dims": {
        "input": 10,
        "output": 3,
        "output_after_transform": 3
    },
    "loss": {
        "otype": "RelativeL2Luminance",
        "luminance_weighting": false,
        "custom_weighting": false,
        "tonemapped": false,
        "lum_weight": 1.0,
        "var_weight": 0.0,
        "luminance_divider": 0.01
    },
    "optimizer": {

        "otype": "ExponentialDecay",
        "nested":
        {
            "otype": "Adam",
            "learning_rate": 0.001,
            "beta1": 0.9,
            "beta2": 0.99,  
            "l2_reg": 0.00001,
            "beta3": 0.9,
            "beta_shampoo": 0.0,
            "identity": 0.0001,
